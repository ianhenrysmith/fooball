%nav.navbar.navbar-inverse.navbar-static-top{ role: 'navigation'}
  .container-fluid
    .navbar-header
      = link_to "fooball", root_path, class: 'navbar-brand'

    %ul.nav.navbar-nav

    %ul.nav.navbar-nav.navbar-right
      - if current_user
        %li= link_to 'Your Profile', user_url(current_user)
        %li= link_to 'Sign Out', logout_path
      - else
        %li
          = link_to "Sign in with Google", user_omniauth_authorize_path(:google_oauth2)